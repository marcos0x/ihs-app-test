{
  "expo": {
    "name": "IHS Markit",
    "slug": "ihsapptest",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "scheme": "ihsapptest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "associatedDomains": [
        "10.65.10.96:8080"
      ],
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["ihsapptest"]
      }
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "MAIN",
          "category": [
            "LAUNCHER"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "http",
              "host": "10.65.10.96:8080",
              "pathPrefix": ""
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "ihsapptest"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]      
    }
  }
}
